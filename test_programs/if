a=1;
b=0;
c=-1;

write("a: ", a);
write("\n");
write("b: ", b);
write("\n");
write("c: ", c);
write("\n");

if(a){
	write("a es mayor que 0\n");
	n=10;
	if(a){
		write("a es mayor que 0\n");
	}
	write("this gets executed");
}else{
	write("a es menor o igual a 0\n");
}

if(b){
	write("b es mayor que 0\n");
}else{
	write("b es menor o igual a 0\n");
}

if(2*a-1+c){
	write("2*a-1+c es mayor que 0\n");
}else{
	write("2*a-1+c es menor o igual a 0\n");
}